{
  "name": "fractal-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@atlaskit/button": "^16.2.0",
    "@atlaskit/checkbox": "^12.3.9",
    "@atlaskit/drawer": "^7.1.5",
    "@atlaskit/empty-state": "^7.3.7",
    "@atlaskit/form": "^8.5.1",
    "@atlaskit/icon": "^21.10.4",
    "@atlaskit/page-header": "^10.2.2",
    "@atlaskit/progress-bar": "^0.5.1",
    "@atlaskit/range": "^6.0.1",
    "@atlaskit/select": "^15.2.9",
    "@atlaskit/textfield": "^5.1.7",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "clsx": "^1.1.1",
    "effector": "^22.2.0",
    "effector-react": "^22.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sass": "^1.49.9",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start & yarn run start:worker",
    "start:worker": "esbuild ./src/webWorker/index.js --outfile=public/fractal.js --target=esnext --allow-overwrite --bundle --watch",
    "prebuild": "yarn run build:worker",
    "build": "react-scripts build",
    "build:worker": "esbuild ./src/webWorker/index.js --outfile=public/fractal.js --target=esnext --allow-overwrite --bundle --minify",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "esbuild": "^0.14.25",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "prettier": "^2.5.1"
  },
  "packageManager": "yarn@3.2.0"
}
